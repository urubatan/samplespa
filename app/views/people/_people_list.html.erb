<%= turbo_frame_tag 'people_list' do %>
<ul>
  <% @people.each do |person| %>
    <li>
      <%= link_to person.name, person, data: {turbo_stream: true, turbo_method: 'get'} %>
    </li>
  <% end %>
  <li>
    <%= link_to 'Create Person Record', new_person_path, data: {turbo_stream: true, turbo_method: 'get'} %>
  </li>
</ul>
<% end %>